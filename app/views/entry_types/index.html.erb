<h1>Listing entry_types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Slug</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @entry_types.each do |entry_type| %>
      <tr>
        <td><%= entry_type.name %></td>
        <td><%= entry_type.description %></td>
        <td><%= entry_type.slug %></td>
        <td><%= link_to 'Show', entry_type %></td>
        <td><%= link_to 'Edit', edit_entry_type_path(entry_type) %></td>
        <td><%= link_to 'Destroy', entry_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Entry type', new_entry_type_path %>
